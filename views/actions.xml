<?xml version="1.0"?>
<openerp>
    <data>
        
        <record model="ir.actions.act_window" id="certificationv01.act_regedit">
            <field name="name">Register certification</field>
            <field name="res_model">registr</field>
            <field name="view_mode">tree,form,calendar,gantt</field>
            <field name="view_type">form</field>
            
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
            Click to create a new register certification.
          </p>
        
            </field>
        </record>
		
        <record model="ir.actions.act_window" id="certificationv01.new_act">
