            <field name="res_model">res.partner</field>
            <field name="context">{'search_default_organ':1,'default_customer': 0,'default_organ': 1}</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_type">form</field>
            <field name="target">current</field>
            
        </record>
		
		     <record model="ir.actions.act_window" id="certificationv01.act_status">
            <field name="name">Status certificate</field>
            <field name="res_model">status</field>
